<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.Pane?>


<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Pane prefHeight="500.0" prefWidth="500.0">
         <children>
            <Button fx:id="playButton" layoutX="372.0" layoutY="14.0" mnemonicParsing="false" onAction="#playGame" text="PLAY" textFill="#05c33b">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="30.0" />
               </font>
            </Button>
            <Button fx:id="quitButton" layoutX="360.0" layoutY="424.0" mnemonicParsing="false" onAction="#quitApp" text="QUIT">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="30.0" />
               </font>
            </Button>
            <Button fx:id="rulesButton" layoutX="348.0" layoutY="104.0" mnemonicParsing="false" onAction="#displayRules" text="RULES" textFill="#fc2323">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="30.0" />
               </font>
            </Button>
            <Button fx:id="tutorialButton" layoutX="14.0" layoutY="424.0" mnemonicParsing="false" onAction="#startTutorial" text="TUTORIAL" textFill="#1000f2">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="30.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</Pane>
